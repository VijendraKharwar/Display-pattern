/*
0 1 2 3 4 0 
1 0 3 4 0 6
1 2 0 0 5 6
1 2 0 0 5 6
1 0 3 4 0 6
0 2 3 4 5 0
*/



class J 
{
	public static void main(String[] args) 
	{
		for(int i = 1; i <= 6; i ++)
		{
			for(int j = 1; j <= 6; j++)
			{
					System.out.print((i == j || ((i + j) == 7) ? 0 : j) + " ");
			}
			System.out.println();
		}
	}
}
